   <script type="text/javascript">

$(function(){
	
	
	$('.favoriteMessage').click(function() {
	
		$(this).fadeOut("slow");
	});
	
	var rowCount = $('#downloadDetailsTable tr').length;
	
	if (rowCount > 6) {
		$('#showMore').show();
		
		$('#downloadDetailsTable tr').each(function() {
		
			if ($(this).index() > 5) {
				$(this).hide();
			}
		});
	}
	
	$('#showMore').click(function() {
	
		$('#downloadDetailsTable tr').each(function() {
		
			if ($(this).index() > 5) {
				$(this).show('fast');
			}
		});
	
		$('#showMore').hide();
		$('#showLess').show();
	});
	
	$('#showLess').click(function() {
	
		$('#downloadDetailsTable tr').each(function() {
		
			if ($(this).index() > 5) {
				$(this).hide('fast');
			}
		});
	
		$('#showMore').show();
		$('#showLess').hide();
	});
	
	$('.sc-favoriteFull').qtip({
		content:'Add page to my favorites',
		style: {classes: 'qtip-white'},
		position: {
			at : 'middle bottom', 
			my : 'top left',
			effect: false,
			adjust: {
        		x: 10,
        		y: 10
        	}
		}
	});
	
	$('.favoriteLink').qtip({
		content:'View All My Favorites',
		style: {classes: 'qtip-white'},
		position: {
			at : 'middle bottom', 
			my : 'top left',
			effect: false,
			adjust: {
       			x: 10,
        		y: 10
        	}
		}
	});
});

var itemId = "7385";
var timer;

function initFavorites() {

	$('.sc-favoriteFull').qtip('option', 'content.text', 'Add page to my favorites');
	
	$('.sc-favoriteFull').click(function(event) {
		
		if ($('.sc-favorite').hasClass("favorite")) {
		
			var favId = $('.sc-favorite').attr("favId");
			removeFromFavorites(itemId, favId, "Downloads");

			
		} else {
				
			var title = $("#downloadTitleDiv").text().replace(/^(\s+)|(\s+)$/g, "");
			addToFavorites(itemId, title, "Downloads");
		}
		
	});
	
	$.ajax({
		type:					"post",
		url:					"dojoXhr",
		data:					{
									action : "favorites",
									itemId: itemId,
									type: "Downloads",
									description: "",
									subAction: "checkFavorite",
									commented:"false" 
								},
		cached:					"false",
		dataType:				"json",
		success: function(response){
	
			if (response.favoriteError != "true") {
			
				var isFavorite = response.isFavorite;
				
				if (isFavorite == "true") {
					$('.sc-favorite').addClass( "favorite" );
					$('.sc-favorite').attr( 'favId', response.favId );
					$('.sc-favoriteFull').qtip('option', 'content.text', 'Remove page from my favorites');
				}
			
			}
		},
		error:function(){
			
		}
	});
}

function addToFavorites(itemId, title, type) {
	
	$.ajax({	
		type:					"post",
		url:					"dojoXhr",
		data:					{
									action : "favorites",
									itemId: itemId,
									title: title,
									type: type,
									description: "",
									subAction: "addFavorite",
									commented:"false" 
								},
		cached:					"false",
		dataType:				"json",
		success: function(response){
	
			if (response.favoriteError == "true"){
				alert("Failed adding item to favorites. Please try again later.");
			}
			else{
				
				$('.sc-favorite').addClass( "favorite" );
				$('.sc-favorite').attr( 'favId', response.favId );
				$('.sc-favoriteFull').qtip('option', 'content.text', 'Remove page from my favorites');
				$('.favoriteMessage').text("This item was successfully added to your favorites");
				if ($('.favoriteMessage').css('display') == 'none') {
					$('.favoriteMessage').fadeIn("slow");
					timer = setTimeout(function(){$('.favoriteMessage').fadeOut("slow")}, 6000);
				} else {
					clearTimeout(timer);
					timer = setTimeout(function(){$('.favoriteMessage').fadeOut("slow")}, 6000);
				}
			}
		},
		error:function(){
			alert("Failed adding item to favorites. Please try again later.");
		}
	});
}

function removeFromFavorites(itemId, id, type) {
	
	$.ajax({	
		type:					"post",
		url:					"dojoXhr",
		data:					{
									action : "favorites",
									favoriteId: id,
									type: type,
									subAction: "removeFavorite",
									commented:"false" 
								},
		cached:					"false",
		dataType:				"json",
		success: function(response){
	
			if (response.favoriteError == "true"){
				alert("Failed removing item from favorites. Please try again later.");
			}
			else{
		
				$('.sc-favorite').removeClass( "favorite" );
				$('.sc-favorite').attr( 'favId', null );
				$('.sc-favoriteFull').qtip('option', 'content.text', 'Add page to my favorites');
				$('.favoriteMessage').text("This item was removed from your favorites");
				if ($('.favoriteMessage').css('display') == 'none') {
					$('.favoriteMessage').fadeIn("slow");
					timer = setTimeout(function(){$('.favoriteMessage').fadeOut("slow")}, 6000);
				} else {
					clearTimeout(timer);
					timer = setTimeout(function(){$('.favoriteMessage').fadeOut("slow")}, 6000);
				}
			}
		},
		error:function(){
			alert("Failed removing item from favorites. Please try again later.");
		}
	});
}

