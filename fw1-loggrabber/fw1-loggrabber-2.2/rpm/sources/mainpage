<script type="text/javascript" src="https://sc1.checkpoint.com/uc/rebranding15/js/jquery.placeholder.min.js"></script>

<script type="text/javascript">
	
	function setSearch() {
		try {       
			var subm = document.createElement('input');
			subm.setAttribute('name', 'eventSubmit_doSearch');
			subm.setAttribute('id', 'eventSubmit_doSearch');
			subm.setAttribute('type', 'hidden');
 
			var productId = document.getElementById('productId');
			var product = null;
			if (productId && productId.value && productId.value!='' && productMap[productId.value]!=null){
				product = document.createElement('input');
				product.setAttribute('name', 'searchedProduct');
				product.setAttribute('value', productId.value);
				product.setAttribute('type', 'hidden');
			}
 
			var form = null;

			try {
				form = document.searchform;
			} catch(e){}			 

			if (form != null) {
				form.appendChild(subm);	 
				if (product)
					form.appendChild(product);
			}
			
			return false;
		} catch(e){}
	}

	
	function triggrSearchExternal() {
		var submitButton = document.getElementById("eventSubmit_doSearch");
		if (submitButton != null) {
			submitButton.click();
		}
	}

	
	$(document).ready(function() {
	
		var inputWidth = $(".scSearchInputWrap").width();
		var searchButtonLeft = inputWidth - 30;
		$('.scSearchInputButton').css("left", searchButtonLeft + "px");
		$("#customSearch").css("width",inputWidth + "px");
	
		$('#keyWordsInput').attr("placeholder", "Search Support Center");
		$('input').placeholder();
		
		if ($('#keyWordsInput').length>0){
			$('#keyWordsInput').autocomplete({
				source: function(request, response) {
							$.get('/supportcenter/autocomplete', { q: request.term , tab:$('#currentTab').val(), limit:10}, function(data) {
								var databack = data.split('\n');
								if (databack.length<=10)
									response(databack.slice(0,databack.length-1));
								else
									response(databack.slice(0, 10));
							});
						},
				minLength: 2,
				search:function(){
					keepOpen=false;
					$('.scSearchInputButton').hide();
					$('#keyWordsInput').addClass('ui-autocomplete-loading');
					var autocompleteLocation = inputWidth - 20;
					$('.ui-autocomplete-loading').css('background-position', autocompleteLocation + 'px 7px'); 
				},
				response: function() {
					$('.scSearchInputButton').show();
				},
				open:function(){
					$("#keyWordsInput").autocomplete("widget").position({
						my: "left top",
						at: "left bottom",
						of: $(".scSearchInputWrap.scSearchInputCustom"),
						offset: "0",
						collision: "none"
					});
					$("#keyWordsInput").autocomplete("widget").css("width",inputWidth + "px");
					$("#customSearch").appendTo($("#ac-holder"));
					$("#customSearch").position({
						my: "left top",
						at: "left bottom",
						of: $("#keyWordsInput").autocomplete("widget"),
						offset:"0 -2",
						collision: "none"
						
					});
				},
				select:function(e,ui){
					keepOpen=false;
					$('#keyWordsInput').val(ui.item.label);
					setSearch();
					document.forms.searchform.submit();
				},
				close:function(){
					if (keepOpen){
						$("#keyWordsInput").autocomplete("widget").show();
						keepOpen=false;
					}
					else{
						$("#customSearch").appendTo($("#sc-select-holder"));
						$("#customSearch").position({
							my: "left top",
							at: "left bottom",
							of: $("#keyWordsInput"),
							offset:"-1",
						collision: "none"
						});
					}
				},create:function(){
					$(document).append('<div id = "ac-holder" style="position:absolute"></div>');
					$("#keyWordsInput").autocomplete("widget").appendTo("#ac-holder");
					$("#sc-select-holder").appendTo(document.body);
				}
			}).data( "ui-autocomplete" )._renderItem = function( ul, item) {
				var re = new RegExp("^" + this.term) ;
				var t = item.label.replace(re,"<span style='font-weight:bold;color:#333333;'>" + this.term + "</span>");
				var classname = "ac_even";
				if (item.index % 2 == 0)
					classname = "ac_odd";
				if (item.label == "")
					return;
	
				return $( "<li class='"+classname+"'></li>" )
					.data( "ui-autocomplete-item", item )
					.append( "<a>" + t + "</a>" )
					.appendTo( ul );
			};
	      
			$.ui.autocomplete.prototype._renderMenu = function( ul, items ) {
				var self = this;
				$.each( items, function( index, item ) {
					item.index=index;
					self._renderItem( ul, item );
				});
				$(ul).css("border-color","rgb(227, 227, 227)");
				$(ul).css("z-index","10");
			}
		};
		
		
		var selectedProduct = "";
		var productId = $('#productId').val();
		selectedProduct = productId;
		var productName = productMap[productId];
		if (productId==null || productId=='' || productName == null || productName=='')
			return;
			

		$('#productSearchField').text('Search within '+productName);
		
        $('#customSearch div').click(
        function(e){
			$('#customSearch div').each(
		 		function(index, Element){
				 	$(Element).removeClass('searchSelected');
				 });
			$(this).addClass('searchSelected');
			if ($(this).attr('id')=='productSearchField')
				$('#productId').val(selectedProduct);
			else 
				$('#productId').val("");
		
		});
		$('#customSearch').click(
			function(e){
				keepOpen=true;
				e.stopPropagation();
			}
			
		);

		$(document).click(function(e){
			keepOpen=false;
		    $("#customSearch").hide();
		    $("#keyWordsInput").autocomplete("close");
	
		});
			
		$("#keyWordsInput,.scSearchInputWrap").click(function(e){
			if ($("#keyWordsInput").autocomplete("widget").css("display")=='none'){
				$("#customSearch").show();
				$("#customSearch").position({
					my: "left top",
					at: "left bottom",
					of: $("#keyWordsInput"),
					offset:"-1",
				collision: "none"
				});
			}
			$('#keyWordsInput').autocomplete('search');
		    e.stopPropagation();
		});	
	});

	
	$.ajax({
		  url: 'autocomplete?init=1',
		  success: function(data) { 
			console.log('init ok');
		  }
	}); 
	
</script>
  











<script type=text/javascript>

